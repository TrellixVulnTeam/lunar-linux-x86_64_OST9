          MODULE=libktorrent
         VERSION=2.1.0
          SOURCE=$MODULE-$VERSION.tar.gz
 SOURCE_URL_FULL=https://github.com/KDE/libktorrent/archive/v$VERSION.tar.gz
   MODULE_PREFIX=${KDE4_INSTALL_DIR:-/usr}
      SOURCE_VFY=sha256:1d0f2978dccc5b8ccd274324b515fbcde8ca9481666b1c2f761e157110de035e
