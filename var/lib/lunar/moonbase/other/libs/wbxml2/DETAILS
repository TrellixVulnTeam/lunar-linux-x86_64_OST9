         SOURCE2=wbxml2-0.9.2.make_install.patch
      SOURCE_URL=$SFORGE_URL/wbxmllib/wbxml2/0.9.2/
     SOURCE2_URL=http://sources.gentoo.org/cgi-bin/viewvc.cgi/gentoo-x86/dev-libs/libwbxml/files/
