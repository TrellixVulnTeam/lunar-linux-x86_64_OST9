default_config &&

make &&
prepare_install &&

mkdir -p /usr/bin /usr/share/man/man8 &&
install -c -m 755 powerd /usr/bin/powerd &&
install -c -m 755 detectups /usr/bin/detectups &&
install -c -m 644 powerd.8 /usr/share/man/man8/powerd.8 &&
install -c -m 644 detectups.8 /usr/share/man/man8/detectups.8
